    "@testing-library/react": "14.0.0",
    "@vvo/tzdb": "^6.157.0",
    "autoprefixer": "^10.4.13",
    "body-scroll-lock": "^4.0.0-beta.0",
    "contentlayer2": "0.5.3",
    "date-fns": "^4.1.0",
