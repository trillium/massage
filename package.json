{
  "name": "trillium-massage",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "next dev",
    "dev": "cross-env INIT_CWD=$PWD next dev --port 9876",
    "build": "cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "build:staged": "./scripts/build-staged.sh",
    "build:staged:verbose": "./scripts/build-staged.sh --verbose",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "eslint --fix app components lib layouts scripts",
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "ratings:spellcheck": "ts-node scripts/ratingsCorrectionScript.ts",
    "spellcheck": "cspell '**/*.{ts,tsx,js,jsx,md,mdx}' --no-progress",
    "spellcheck:fix": "cspell '**/*.{ts,tsx,js,jsx,md,mdx}' --no-progress --show-suggestions",
    "commit:build": "git config hooks.prebuild true && git commit",
    "commit:no-build": "git config hooks.prebuild false && git commit",
    "generate-static-map": "ts-node scripts/generate-la-map-auto.ts",
    "cspell:ignore": "./scripts/cspell-ignore-and-prettier.sh",
    "convert-images": "bash ./scripts/convert_and_resize_images.sh",
    "admin:generate-link": "tsx scripts/generate-admin-link.ts --emailEnv",
    "user:generate-link": "tsx scripts/generate-user-link.ts",
    "push:send-test-message": "tsx scripts/send-test-pushover.ts"
  },
  "dependencies": {
    "@headlessui/react": "2.2.9",
    "@heroicons/react": "^2.2.0",
    "@next/bundle-analyzer": "15.5.4",
    "@reduxjs/toolkit": "^2.9.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/react": "14.0.0",
    "@vis.gl/react-maplibre": "^8.0.4",
    "@vvo/tzdb": "^6.184.0",
    "autoprefixer": "^10.4.13",
    "body-scroll-lock": "^4.0.0-beta.0",
    "contentlayer2": "0.5.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.2",
    "esbuild": "0.25.10",
    "formik": "^2.4.6",
    "github-slugger": "^2.0.0",
    "googleapis": "^157.0.0",
    "gray-matter": "^4.0.2",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "image-size": "2.0.2",
    "lru-cache": "^11.2.2",
    "maplibre-gl": "^5.6.2",
    "next": "15.5.4",
    "next-contentlayer2": "0.5.5",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.9.16",
    "pliny": "0.4.1",
    "postcss": "^8.4.24",
    "posthog-js": "^1.258.2",
    "posthog-node": "^4.18.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.3.0",
    "rehype-katex": "^7.0.0",
    "rehype-katex-notranslate": "^1.1.4",
    "rehype-preset-minify": "7.0.1",
    "rehype-prism-plus": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.0",
    "remark-gfm": "^4.0.0",
    "remark-github-blockquote-alert": "^1.2.1",
    "remark-math": "^6.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.0.5",
    "unist-util-visit": "^5.0.0",
    "wicg-inert": "^3.1.3",
    "yup": "^1.7.1",
    "zod": "^4.0.10",
    "zod-formik-adapter": "^1.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.36.0",
    "@faker-js/faker": "^9.3.0",
    "@jest/types": "^30.2.0",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/mdx": "^2.0.12",
    "@types/react": "^19.1.15",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "cli-progress": "^3.12.0",
    "cross-env": "^10.1.0",
    "cspell": "^9.2.1",
    "dictionary-en": "^4.0.0",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.3",
    "msw": "^2.11.3",
    "nspell": "^2.1.5",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "puppeteer": "^24.16.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix",
      "bash -c 'cspell --no-progress \"$@\" || exit 0' --"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "bash -c 'cspell --no-progress \"$@\" || exit 0' --"
    ]
  },
  "packageManager": "pnpm@9.15.0"
}
