    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@faker-js/faker": "^9.3.0",
    "@jest/types": "^29.6.3",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/mdx": "^2.0.12",
    "@types/react": "^18.2.73",
